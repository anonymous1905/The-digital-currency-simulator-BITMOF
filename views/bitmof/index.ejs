<% let count = 0 %>

<!-- <div class="container-fluid"> -->

<h2> List of all Digital Currencies we Offer </h2>
<br>
<div class=>
  <br>
<h2> COIN </h2>
</div>

<% bitmofObjects.forEach((onebitmof) => { %>

<div class="allcoins">
  <div class="row">
    <div class="coins">
   <h3><%= onebitmof.coinTicker  %> </h3>
  <img id='logo' src = "<%= onebitmof.coinImage %>"> </img>
</div>
<div class="coins">
   <h3> <%= onebitmof.coinName %>  </h3>
     <br>
   </div>
   <div class="coins">
        Founded in: <h6> <%= onebitmof.coinDate %></h6>
       <br>
     </div>
     <div class="coins">
       by: <h6><%= onebitmof.coinFounder %></h6>
     </div>
    <p> THE PRICE: </p>
    <p>$<%= pricesObjects[count] %></p>
    <% count +=1%>
      <hr>

      </div>
    </div>
        <% });  %>
